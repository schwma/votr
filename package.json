{
  "name": "votr",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node src/server.js",
    "test": "NODE_ENV=test jest --runInBand",
    "format:test": "prettier --list-different \"{src,test,__tests__,config,docs}/**/*.{js,json,css,md}\"",
    "format": "prettier --write \"{src,test,__tests__,config,docs}/**/*.{js,json,css,md}\"",
    "coverage": "NODE_ENV=test nyc mocha --recursive",
    "coverage-html": "NODE_ENV=test nyc --reporter=html mocha --recursive"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "Marcel Schwarz",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.5.5",
    "crypto-extra": "^0.4.0",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "sequelize": "^4.44.3",
    "sqlite3": "^4.0.8"
  },
  "devDependencies": {
    "husky": "^1.3.1",
    "jest": "^24.9.0",
    "lint-staged": "^8.1.7",
    "prettier": "1.16.4",
    "supertest": "^3.4.2"
  }
}
